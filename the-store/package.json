{
  "name": "the-store",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "postinstall": "patch-package",
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && npm run build:server && npm run build:admin",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.server.json",
    "build:admin": "cross-env medusa-admin build",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "seed": "cross-env medusa seed -f ./data/seed.json",
    "start": "cross-env npm run build && medusa start",
    "start:custom": "cross-env npm run build && node --preserve-symlinks --trace-warnings index.js",
    "dev": "cross-env npm run build:server && medusa develop",
    "migration": "medusa migrations run"
  },
  "dependencies": {
    "@medusajs/admin": "7.1.10",
    "@medusajs/cache-inmemory": "^1.8.10",
    "@medusajs/cache-redis": "^1.9.0",
    "@medusajs/event-bus-local": "^1.9.8",
    "@medusajs/event-bus-redis": "^1.8.11",
    "@medusajs/file-local": "^1.0.3",
    "@medusajs/medusa": "1.20.3",
    "@tanstack/react-query": "4.22.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "16.3.1",
    "express": "^4.19.0",
    "marked": "^12.0.1",
    "medusa-auth-otp": "file:../packages/medusa-auth-otp",
    "medusa-core-utils": "^1.2.1",
    "medusa-file-s3": "^1.4.1",
    "medusa-fulfillment-manual": "^1.1.40",
    "medusa-interfaces": "^1.3.9",
    "medusa-novaposhta-fulfillment": "file:../packages/medusa-novaposhta-fulfillment",
    "medusa-payment-manual": "^1.0.24",
    "medusa-payment-stripe": "^6.0.8",
    "prism-react-renderer": "^2.3.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-typescript": "^7.24.1",
    "@medusajs/medusa-cli": "^1.3.22",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.5.1",
    "patch-package": "^8.0.0",
    "rimraf": "^3.0.2",
    "swc-loader": "^0.2.6",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "commons": "file:../packages/commons"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
